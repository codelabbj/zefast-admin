# Chart Colors (oklch format)
NEXT_PUBLIC_CHART_1="oklch(0.62 0.21 35)"
NEXT_PUBLIC_CHART_2="oklch(0.68 0.23 30)"
NEXT_PUBLIC_CHART_3="oklch(0.58 0.19 40)"
NEXT_PUBLIC_CHART_4="oklch(0.65 0.20 25)"
NEXT_PUBLIC_CHART_5="oklch(0.70 0.22 45)"

# Feature Flags - Enable/Disable Pages (true/false)
NEXT_PUBLIC_ENABLE_USERS="true"
NEXT_PUBLIC_ENABLE_BOT_USERS="true"
NEXT_PUBLIC_ENABLE_NETWORKS="true"
NEXT_PUBLIC_ENABLE_TELEPHONES="true"
NEXT_PUBLIC_ENABLE_USER_APP_IDS="true"
NEXT_PUBLIC_ENABLE_NOTIFICATIONS="true"
NEXT_PUBLIC_ENABLE_BONUSES="true"
NEXT_PUBLIC_ENABLE_COUPONS="true"
NEXT_PUBLIC_ENABLE_ADVERTISEMENTS="true"
NEXT_PUBLIC_ENABLE_TRANSACTIONS="true"
NEXT_PUBLIC_ENABLE_RECHARGES="true"
NEXT_PUBLIC_ENABLE_BOT_TRANSACTIONS="true"
NEXT_PUBLIC_ENABLE_PLATFORMS="true"
NEXT_PUBLIC_ENABLE_DEPOSITS="false"
NEXT_PUBLIC_ENABLE_SETTINGS="true"
NEXT_PUBLIC_API_CHOICES="connect:Cashika Connect"



